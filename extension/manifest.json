{
    "name": "React Chrome Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": [
          "*://*/*"
        ],
        "js": [
          "content.js"
        ]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }